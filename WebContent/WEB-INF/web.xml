<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Progetto TSW</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  
  <listener>
        <listener-class>model.DataSourceInizialize</listener-class>
    </listener>
  
  <servlet>
    <servlet-name>BestsellerCarousel</servlet-name>
    <servlet-class>it.unisa.Controller.BestsellerCarousel</servlet-class>
  </servlet>

  <!-- Servlet Mapping -->
  <servlet-mapping>
    <servlet-name>BestsellerCarousel</servlet-name>
    <url-pattern>/carousel</url-pattern>
  </servlet-mapping>

  
</web-app>